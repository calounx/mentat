{
  "dashboard": {
    "title": "CHOM - System Overview",
    "tags": ["chom", "overview", "production"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(chom_laravel_http_requests_total[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "req/s", "format": "short"},
          {"format": "short"}
        ],
        "legend": {"show": true}
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(chom_laravel_http_requests_errors_total[5m])) / sum(rate(chom_laravel_http_requests_total[5m])) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "%", "format": "percent"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [1], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Error Rate",
          "noDataState": "no_data"
        }
      },
      {
        "id": 3,
        "title": "Request Duration (p95)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(chom_laravel_http_request_duration_seconds_bucket[5m])) by (le, route))",
            "legendFormat": "{{route}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "seconds", "format": "s"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Active Requests",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "chom_laravel_http_requests_active",
            "legendFormat": "Active Requests",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "requests", "format": "short"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "chom_laravel_memory_usage_mb",
            "legendFormat": "Memory MB",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "MB", "format": "decmbytes"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(chom_laravel_cache_operations_total{operation=\"hit\"}[5m])) / (sum(rate(chom_laravel_cache_operations_total{operation=\"hit\"}[5m])) + sum(rate(chom_laravel_cache_operations_total{operation=\"miss\"}[5m]))) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "%", "format": "percent"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Top Slow Endpoints (p95)",
        "type": "table",
        "gridPos": {"x": 0, "y": 24, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, sum(rate(chom_laravel_http_request_duration_seconds_bucket[5m])) by (le, route)))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "route": "Route",
                "Value": "Duration (s)"
              }
            }
          }
        ]
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
