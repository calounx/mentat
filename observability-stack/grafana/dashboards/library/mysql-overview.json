{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [{ "expr": "mysql_up{instance=~\"$instance\"}", "refId": "A" }],
      "title": "MySQL Status",
      "type": "stat",
      "fieldConfig": {
        "defaults": {
          "mappings": [{ "options": { "0": { "color": "red", "text": "DOWN" }, "1": { "color": "green", "text": "UP" } }, "type": "value" }],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 1 }] }
        }
      }
    },
    {
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "id": 2,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "targets": [{ "expr": "mysql_global_status_threads_connected{instance=~\"$instance\"}", "refId": "A" }],
      "title": "Connections",
      "type": "stat",
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
      "id": 3,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "targets": [{ "expr": "rate(mysql_global_status_queries{instance=~\"$instance\"}[5m])", "refId": "A" }],
      "title": "Queries/sec",
      "type": "stat",
      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "id": 4,
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "targets": [{ "expr": "rate(mysql_global_status_slow_queries{instance=~\"$instance\"}[5m])", "refId": "A" }],
      "title": "Slow Queries/sec",
      "type": "stat",
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.01 }, { "color": "red", "value": 0.1 }] }
        }
      }
    },
    {
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "id": 5,
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "targets": [
        {
          "expr": "(rate(mysql_global_status_innodb_buffer_pool_read_requests{instance=~\"$instance\"}[5m]) - rate(mysql_global_status_innodb_buffer_pool_reads{instance=~\"$instance\"}[5m])) / rate(mysql_global_status_innodb_buffer_pool_read_requests{instance=~\"$instance\"}[5m]) * 100",
          "refId": "A"
        }
      ],
      "title": "Buffer Pool Hit Rate",
      "type": "stat",
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "yellow", "value": 95 }, { "color": "green", "value": 99 }] }
        }
      }
    },
    {
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "id": 6,
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "targets": [
        {
          "expr": "mysql_global_status_threads_connected{instance=~\"$instance\"} / mysql_global_variables_max_connections{instance=~\"$instance\"} * 100",
          "refId": "A"
        }
      ],
      "title": "Connection Usage",
      "type": "stat",
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 70 }, { "color": "red", "value": 90 }] }
        }
      }
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "id": 7,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "mysql_global_status_threads_connected{instance=~\"$instance\"}", "legendFormat": "Connected", "refId": "A" },
        { "expr": "mysql_global_status_threads_running{instance=~\"$instance\"}", "legendFormat": "Running", "refId": "B" },
        { "expr": "mysql_global_status_threads_cached{instance=~\"$instance\"}", "legendFormat": "Cached", "refId": "C" }
      ],
      "title": "Thread Activity",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "id": 8,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "rate(mysql_global_status_com_select{instance=~\"$instance\"}[5m])", "legendFormat": "SELECT", "refId": "A" },
        { "expr": "rate(mysql_global_status_com_insert{instance=~\"$instance\"}[5m])", "legendFormat": "INSERT", "refId": "B" },
        { "expr": "rate(mysql_global_status_com_update{instance=~\"$instance\"}[5m])", "legendFormat": "UPDATE", "refId": "C" },
        { "expr": "rate(mysql_global_status_com_delete{instance=~\"$instance\"}[5m])", "legendFormat": "DELETE", "refId": "D" }
      ],
      "title": "Query Types",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "id": 9,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "mysql_global_status_innodb_buffer_pool_bytes_data{instance=~\"$instance\"}", "legendFormat": "Data", "refId": "A" },
        { "expr": "mysql_global_status_innodb_buffer_pool_bytes_dirty{instance=~\"$instance\"}", "legendFormat": "Dirty", "refId": "B" },
        { "expr": "mysql_global_variables_innodb_buffer_pool_size{instance=~\"$instance\"}", "legendFormat": "Pool Size", "refId": "C" }
      ],
      "title": "InnoDB Buffer Pool",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "bytes" } }
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "id": 10,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "rate(mysql_global_status_innodb_rows_read{instance=~\"$instance\"}[5m])", "legendFormat": "Read", "refId": "A" },
        { "expr": "rate(mysql_global_status_innodb_rows_inserted{instance=~\"$instance\"}[5m])", "legendFormat": "Inserted", "refId": "B" },
        { "expr": "rate(mysql_global_status_innodb_rows_updated{instance=~\"$instance\"}[5m])", "legendFormat": "Updated", "refId": "C" },
        { "expr": "rate(mysql_global_status_innodb_rows_deleted{instance=~\"$instance\"}[5m])", "legendFormat": "Deleted", "refId": "D" }
      ],
      "title": "InnoDB Row Operations",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 20 },
      "id": 11,
      "title": "Replication",
      "type": "row"
    },
    {
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 21 },
      "id": 12,
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "targets": [{ "expr": "mysql_slave_status_slave_io_running{instance=~\"$instance\"}", "refId": "A" }],
      "title": "IO Thread",
      "type": "stat",
      "fieldConfig": {
        "defaults": {
          "mappings": [{ "options": { "0": { "color": "red", "text": "Stopped" }, "1": { "color": "green", "text": "Running" } }, "type": "value" }]
        }
      }
    },
    {
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 21 },
      "id": 13,
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "targets": [{ "expr": "mysql_slave_status_slave_sql_running{instance=~\"$instance\"}", "refId": "A" }],
      "title": "SQL Thread",
      "type": "stat",
      "fieldConfig": {
        "defaults": {
          "mappings": [{ "options": { "0": { "color": "red", "text": "Stopped" }, "1": { "color": "green", "text": "Running" } }, "type": "value" }]
        }
      }
    },
    {
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 21 },
      "id": 14,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "targets": [{ "expr": "mysql_slave_status_seconds_behind_master{instance=~\"$instance\"}", "refId": "A" }],
      "title": "Replication Lag",
      "type": "stat",
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 30 }, { "color": "red", "value": 60 }] }
        }
      }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
