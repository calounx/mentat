        "multi": true,
        "name": "instance",
        "options": [],
        "query": { "query": "label_values(up{job=\"alloy\"}, instance)", "refId": "StandardVariableQuery" },
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "title": "Grafana Alloy Overview",
  "uid": "alloy-overview-lib",
