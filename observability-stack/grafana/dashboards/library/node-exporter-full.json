      "targets": [{ "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\",instance=\"$instance\"}[5m])) * 100)", "legendFormat": "CPU" }],
      "title": "CPU Usage",
      "type": "gauge"
