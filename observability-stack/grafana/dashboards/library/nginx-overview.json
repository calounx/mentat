{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "expr": "nginx_up{instance=~\"$instance\"}",
          "refId": "A"
        }
      ],
      "title": "Nginx Status",
      "type": "stat",
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "options": { "0": { "color": "red", "text": "DOWN" }, "1": { "color": "green", "text": "UP" } }, "type": "value" }
          ],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 1 }] }
        }
      }
    },
    {
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(rate(nginx_http_requests_total{instance=~\"$instance\"}[5m]))",
          "refId": "A"
        }
      ],
      "title": "Requests/sec",
      "type": "stat",
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },
    {
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "id": 3,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "nginx_connections_active{instance=~\"$instance\"}",
          "refId": "A"
        }
      ],
      "title": "Active Connections",
      "type": "stat",
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "id": 4,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(rate(nginx_http_requests_total{instance=~\"$instance\", status=~\"5..\"}[5m])) / sum(rate(nginx_http_requests_total{instance=~\"$instance\"}[5m])) * 100",
          "refId": "A"
        }
      ],
      "title": "Error Rate",
      "type": "stat",
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 5 }] }
        }
      }
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "id": 5,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(nginx_http_requests_total{instance=~\"$instance\"}[5m])) by (status)", "legendFormat": "{{status}}", "refId": "A" }
      ],
      "title": "Requests by Status Code",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "reqps", "custom": { "fillOpacity": 20, "stacking": { "mode": "normal" } } } }
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "id": 6,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "nginx_connections_active{instance=~\"$instance\"}", "legendFormat": "Active", "refId": "A" },
        { "expr": "nginx_connections_reading{instance=~\"$instance\"}", "legendFormat": "Reading", "refId": "B" },
        { "expr": "nginx_connections_writing{instance=~\"$instance\"}", "legendFormat": "Writing", "refId": "C" },
        { "expr": "nginx_connections_waiting{instance=~\"$instance\"}", "legendFormat": "Waiting", "refId": "D" }
      ],
      "title": "Connections",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "id": 7,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "rate(nginx_connections_accepted{instance=~\"$instance\"}[5m])", "legendFormat": "Accepted", "refId": "A" },
        { "expr": "rate(nginx_connections_handled{instance=~\"$instance\"}[5m])", "legendFormat": "Handled", "refId": "B" }
      ],
      "title": "Connection Rate",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "cps" } }
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "id": 8,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(nginx_http_request_duration_seconds_bucket{instance=~\"$instance\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(nginx_http_request_duration_seconds_bucket{instance=~\"$instance\"}[5m])) by (le))",
          "legendFormat": "p90",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(nginx_http_request_duration_seconds_bucket{instance=~\"$instance\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "title": "Request Latency",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "s" } }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["nginx", "web", "library"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "All", "value": "$__all" },
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "definition": "label_values(nginx_up, instance)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "instance",
        "options": [],
        "query": { "query": "label_values(nginx_up, instance)", "refId": "StandardVariableQuery" },
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "title": "Nginx Overview",
  "uid": "nginx-overview-lib",
  "version": 1
}
