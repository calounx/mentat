        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Total Storage"
          }
        ]
      },
      "targets": [
        {
          "expr": "topk(20, tenant_database_size_bytes{job=\"chom\",tenant=~\"$tenant\"})",
          "format": "table",
          "instant": true,
          "refId": "A"
        },
        {
          "expr": "topk(20, tenant_file_storage_bytes{job=\"chom\",tenant=~\"$tenant\"})",
          "format": "table",
          "instant": true,
          "refId": "B"
        },
        {
          "expr": "topk(20, (tenant_database_size_bytes{job=\"chom\",tenant=~\"$tenant\"} + tenant_file_storage_bytes{job=\"chom\",tenant=~\"$tenant\"}))",
          "format": "table",
          "instant": true,
          "refId": "C"
        },
        {
          "expr": "topk(20, (tenant_database_size_bytes{job=\"chom\",tenant=~\"$tenant\"} + tenant_file_storage_bytes{job=\"chom\",tenant=~\"$tenant\"}) - (tenant_database_size_bytes{job=\"chom\",tenant=~\"$tenant\"} offset 7d + tenant_file_storage_bytes{job=\"chom\",tenant=~\"$tenant\"} offset 7d))",
          "format": "table",
          "instant": true,
          "refId": "D"
        }
      ],
      "title": "Storage Consumption by Tenant",
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "job": true,
              "__name__": true,
              "instance": true
            },
            "indexByName": {
              "tenant": 0,
              "Value #A": 1,
              "Value #B": 2,
              "Value #C": 3,
              "Value #D": 4
            },
            "renameByName": {
              "Value #A": "Database Size",
              "Value #B": "File Storage",
              "Value #C": "Total Storage",
              "Value #D": "Growth (7d)",
              "tenant": "Tenant"
            }
          }
        }
      ],
      "type": "table"
    },
