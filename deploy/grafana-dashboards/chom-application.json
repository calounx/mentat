{
  "dashboard": {
    "title": "CHOM Application Metrics",
    "tags": ["chom", "application", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Request Rate",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (method, endpoint)",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Response Time (Percentiles)",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p50 - {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p95 - {{endpoint}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p99 - {{endpoint}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Response Time"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 0.5, "colorMode": "custom", "op": "gt", "fill": true, "line": true, "fillColor": "rgba(234, 112, 112, 0.12)", "lineColor": "rgba(237, 129, 40, 0.60)"},
          {"value": 1.0, "colorMode": "custom", "op": "gt", "fill": true, "line": true, "fillColor": "rgba(255, 73, 73, 0.18)", "lineColor": "rgba(255, 73, 73, 1)"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "type": "graph",
        "title": "Error Rate by Status Code",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"4..\"}[5m])) by (status)",
            "legendFormat": "4xx - {{status}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m])) by (status)",
            "legendFormat": "5xx - {{status}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [10], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["B", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "High Error Rate (5xx)",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "type": "graph",
        "title": "Active Users & Sessions",
        "targets": [
          {
            "expr": "laravel_active_users_total",
            "legendFormat": "Active Users",
            "refId": "A"
          },
          {
            "expr": "laravel_active_sessions_total",
            "legendFormat": "Active Sessions",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Count"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "Queue Job Throughput",
        "targets": [
          {
            "expr": "rate(laravel_queue_jobs_processed_total[5m])",
            "legendFormat": "Processed",
            "refId": "A"
          },
          {
            "expr": "rate(laravel_queue_jobs_failed_total[5m])",
            "legendFormat": "Failed",
            "refId": "B"
          },
          {
            "expr": "laravel_queue_size",
            "legendFormat": "Pending",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Jobs/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "Cache Hit Rate",
        "targets": [
          {
            "expr": "rate(laravel_cache_hits_total[5m]) / (rate(laravel_cache_hits_total[5m]) + rate(laravel_cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "max": 100, "min": 0, "label": "Hit Rate"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 50, "colorMode": "custom", "op": "lt", "fill": true, "line": true, "fillColor": "rgba(255, 73, 73, 0.18)", "lineColor": "rgba(255, 73, 73, 1)"},
          {"value": 70, "colorMode": "custom", "op": "lt", "fill": true, "line": true, "fillColor": "rgba(234, 112, 112, 0.12)", "lineColor": "rgba(237, 129, 40, 0.60)"}
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "type": "table",
        "title": "Top 10 Slowest Endpoints",
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint)))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "endpoint": "Endpoint",
                "Value": "p95 Response Time (s)"
              }
            }
          }
        ]
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}
